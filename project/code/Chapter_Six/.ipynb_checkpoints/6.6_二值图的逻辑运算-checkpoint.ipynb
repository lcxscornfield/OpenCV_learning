   "source": [
    "#-*-coding:utf-8  -*-\n",
    "import cv2\n",
    "import numpy as np\n",
    "src1 = np.array([[255,0,255]])\n",
    "src2 = np.array([[255,0,0]])\n",
    "\n",
    "#与运算\n",
    "dst_and = cv2.bitwise_and(src1,src2)\n",
    "#或运算\n",
    "dst_or = cv2.bitwise_or(src1,src2)\n",
    "print dst_and\n",
    "print dst_or"
   ]
  }
